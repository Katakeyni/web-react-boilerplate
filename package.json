{
  "name": "webpack-boilerplate",
  "version": "3.0.0",
  "description": "Plain webpack 3 Boilerplate with Babel, SASS and lodash on board",
  "author": "Christoph von Gellhorn",
  "license": "MIT",
  "homepage": "https://github.com/cvgellhorn/webpack-boilerplate",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --progress --hot --inline --config webpack.config.dev.js",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --config webpack.config.dev.js",
    "build": "webpack -p --progress --config webpack.config.build.js",
    "web-build": "webpack",
    "web-dev": "webpack && cross-env NODE_ENV=dev webpack-dev-server --hot  --progress --config webpack.config.dev.js "
  },
  "repository": {
    "type": "git",
    "url": "https://cvgellhorn@github.com/cvgellhorn/webpack-boilerplate.git"
  },
  "dependencies": {
    "@fengyuanchen/datepicker": "^0.6.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.0.4",
    "css-hot-loader": "^1.3.4",
    "css-loader": "^0.28.7",
    "ejs-loader": "^0.3.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "^1.1.5",
    "fullpage.js": "^2.9.5",
    "gsap": "^1.20.3",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "rellax": "^1.3.0",
    "sass-loader": "^6.0.6",
    "simple-line-icons": "git+https://github.com/Katakeyni/simple-line-icons.git",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "devDependencies": {}
}
